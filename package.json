{
  "name": "wifi-troubleshooting-app-root",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && ./venv/bin/python -m uvicorn app.main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev -- --host"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}